<h2>Show Scraper</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @scraper.name %>
  </li>

  <li>
    <strong>Last content:</strong>
    <%= @scraper.last_content_id %>
  </li>

  <li>
    <strong>Last page:</strong>
    <%= @scraper.last_page_id %>
  </li>

    <li>
    <strong>Current page:</strong>
    <%= @scraper.current_thread_id %>
  </li>

  <%= link "Scrape", to: scraper_scrape_path(@conn, :scrape, @scraper) %>

</ul>

<span><%= link "Edit", to: scraper_path(@conn, :edit, @scraper) %></span>
<span><%= link "Back", to: scraper_path(@conn, :index) %></span>
